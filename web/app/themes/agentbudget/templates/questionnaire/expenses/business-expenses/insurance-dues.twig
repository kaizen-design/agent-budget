<form class="questionnaire-form h-100 py-5 d-none" data-id="{{ id }}" novalidate>
  <div class="container-fluid h-100">
    <div class="row h-100 align-items-center justify-content-center">
      <div class="col-xl-11 col-xxl-8 mt-auto">
        <small class="form-title d-block mb-3">EXPENSES</small>
        <p class="lead text-dark fs-26 mb-4">Enter your projected <strong class="fw-800">insurance & dues</strong> expenses for the next year.</p>
        <div class="row">
          <div class="col-xl-4 mb-3">
            <label class="">E&O insurance</label>
            <input class="form-control form-control-lg"
                   type="text"
                   name="eo_insurance"
                   placeholder="$600"
                   required>
            <small class="d-block text-end fw-700 fs-12 mt-1"><a class="acceptDefaultBtn" href="#">Accept default</a></small>
          </div>
          <div class="col-xl-4 mb-3">
            <label class="">Realtor Dues</label>
            <input class="form-control form-control-lg"
                   type="text"
                   name="realtor_dues"
                   placeholder="$524"
                   required>
            <small class="d-block text-end fw-700 fs-12 mt-1"><a class="acceptDefaultBtn" href="#">Accept default</a></small>
          </div>
          <div class="col-xl-4">
            <label class="">MLS Dues</label>
            <input class="form-control form-control-lg"
                   type="text"
                   name="mls_dues"
                   placeholder="$492"
                   required>
            <small class="d-block text-end fw-700 fs-12 mt-1"><a class="acceptDefaultBtn" href="#">Accept default</a></small>
          </div>
        </div>
        <div class="mt-4 mt-xxl-5">
          <button type="submit" class="btn btn-primary btn-lg mw-90 me-2">OK</button>
          <small class="press-enter-btn text-dark fw-500">Press <strong class="fw-800">Enter â†µ</strong></small>
        </div>
      </div>
      <div class="col-xl-11 col-xxl-8 mt-5 mt-lg-auto">
        {% include 'questionnaire/partials/pagination-progress.twig' %}
      </div>
    </div>
  </div>
</form>